<div id="modal-content" class="modal fade" tabindex="-1" role="dialog">
    <div id="modal-dialog" class="modal-dialog">
        <div class="modal-content">
            <div id="modal-header" class="modal-header">
                <h3 class='end-points'>You got 0 points!</h3>
            </div>
            <div class="modal-body">
              <p class='error-msg'></p>
              <ul>
                <li class='group head'> <span class='name'> Name </span> <span class='highscores'> HighScores </span>
                <% scores.forEach(function(score){ %>
                  <li class='group'>
                    <span class='name'> <%= score.get('name') %> </span> <span class='highscores'> <%=score.get('points') %> </span>
                  </li>
                <% }) %>
              </ul>
              <div class='form-container'>
              </div>
            </div>
            <div id='modal-footer' class="modal-footer">
                 <a href="#" id="modal-button" class="btn btn-primary play-again" >Play Again</a>
            </div>
        </div>
    </div>
</div>
